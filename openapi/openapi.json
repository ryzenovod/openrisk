      "DashboardMetrics": {
        "properties": {
          "total_applications": {
            "type": "integer",
            "title": "Total Applications"
          },
          "approved_rate": {
            "type": "number",
            "title": "Approved Rate"
          },
          "average_pd": {
            "type": "number",
            "title": "Average Pd"
          },
          "portfolio_el": {
            "type": "number",
            "title": "Portfolio El"
          }
        },
        "type": "object",
        "required": [
          "total_applications",
          "approved_rate",
          "average_pd",
          "portfolio_el"
        ],
        "title": "DashboardMetrics"
      },
      "DashboardResponse": {
        "properties": {
          "metrics": {
            "$ref": "#/components/schemas/DashboardMetrics"
          },
          "recent_jobs": {
            "items": {
              "$ref": "#/components/schemas/JobResponse"
            },
            "type": "array",
            "title": "Recent Jobs"
          },
          "pd_distribution": {
            "items": {
              "type": "number"
            },
            "type": "array",
            "title": "Pd Distribution"
          },
          "el_over_time": {
            "items": {
              "type": "number"
            },
            "type": "array",
            "title": "El Over Time"
          },
          "job_durations": {
            "items": {
              "type": "number"
            },
            "type": "array",
            "title": "Job Durations"
          }
        },
        "type": "object",
        "required": [
          "metrics",
          "recent_jobs",
          "pd_distribution",
          "el_over_time",
          "job_durations"
        ],
        "title": "DashboardResponse"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "JobCreateRequest": {
        "properties": {
          "job_type": {
            "type": "string",
            "title": "Job Type"
          },
          "applications": {
            "items": {
              "$ref": "#/components/schemas/ApplicationFeatures"
            },
            "type": "array",
            "title": "Applications",
            "default": []
          },
          "budget": {
            "type": "number",
            "title": "Budget",
            "default": 100000
          }
        },
        "type": "object",
        "required": [
          "job_type"
        ],
        "title": "JobCreateRequest"
      },
      "JobEventResponse": {
        "properties": {
          "id": {
            "type": "integer",
            "title": "Id"
          },
          "job_id": {
            "type": "integer",
            "title": "Job Id"
          },
          "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At"
          },
          "level": {
            "type": "string",
            "title": "Level"
          },
          "message": {
            "type": "string",
            "title": "Message"
          }
        },
        "type": "object",
        "required": [
          "id",
          "job_id",
          "created_at",
          "level",
          "message"
