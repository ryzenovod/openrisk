  "components": {
    "schemas": {
      "ApplicationFeatures": {
        "properties": {
          "age": {
            "type": "integer",
            "maximum": 80.0,
            "minimum": 18.0,
            "title": "Age"
          },
          "income": {
            "type": "number",
            "minimum": 0.0,
            "title": "Income"
          },
          "employment_years": {
            "type": "integer",
            "minimum": 0.0,
            "title": "Employment Years"
          },
          "debt_to_income": {
            "type": "number",
            "maximum": 1.0,
            "minimum": 0.0,
            "title": "Debt To Income"
          },
          "credit_history_months": {
            "type": "integer",
            "minimum": 0.0,
            "title": "Credit History Months"
          },
          "delinquencies": {
            "type": "integer",
            "minimum": 0.0,
            "title": "Delinquencies"
          },
          "loan_amount": {
            "type": "number",
            "minimum": 0.0,
            "title": "Loan Amount"
          },
          "loan_term_months": {
            "type": "integer",
            "minimum": 1.0,
            "title": "Loan Term Months"
          },
          "interest_rate": {
            "type": "number",
            "minimum": 0.0,
            "title": "Interest Rate"
          }
        },
        "type": "object",
        "required": [
          "age",
          "income",
          "employment_years",
          "debt_to_income",
          "credit_history_months",
          "delinquencies",
          "loan_amount",
          "loan_term_months",
          "interest_rate"
